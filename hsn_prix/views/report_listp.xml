<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="minimal_layout_inherit" inherit_id="web.minimal_layout">
		<xpath expr="//head" position="inside">
			<link rel='stylesheet' href="/hsn_prix/static/src/css/prix.css"/>
		</xpath>
</template>
<template id="report_listp">
    <t t-call="web.html_container">
        <t t-set="data_report_margin_top" t-value="12"/>
        <t t-set="data_report_margin_left" t-value="12"/>
        <t t-set="data_report_header_spacing" t-value="9"/>
        <t t-set="data_report_dpi" t-value="110"/>
        <t t-foreach="docs" t-as="o">
            <t t-call="web.basic_layout">
    <div class="header text-left haut">
             <img src= "/hsn_prix/static/src/img/entete.png" width="80%"/>
			</div> 
                <div class="page" style="font-family:Arial, Helvetica, sans-serif; margin-top:30px">
					<div class="tiu">
						 <strong>LISTE DE PRIX <span t-esc="o.name"/> </strong>
                    </div> 
              </div>
    <div class="footer text-center pied">
            BRICO DISCOUNT<br/>
    </div>
        </t>
        </t>
    </t>
</template>
</odoo>

